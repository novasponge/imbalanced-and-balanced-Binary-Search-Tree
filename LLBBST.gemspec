Gem::Specification.new do |s|
  s.name        = 'LLBBST'
  s.version     = '0.0.1'
  s.date        = '2016-12-10'
  s.summary     = "treemap implementation with Left Leaning Red Black Binary Search Tree in Ruby"
  s.description = "Guaranteed O(log(n)) runtime complexity for put, get and delete in all cases by dynamically balancing BST. LLBBST treemap is still under early development. New features will be continuously added in order to support basic treemap functionality."
  s.authors     = ["Zhuoli Zhang"]
  s.email       = 'zhuoli.zhang10@gmail.com'
  s.files       = ["lib/hola.rb"]
  s.homepage    =
    'http://rubygems.org/gems/LLBBST'
  s.license       = 'MIT'
end
